digraph {
	node [shape=circle]
	ROOT -> set
	corpus -> A
	set -> corpus
	corpus -> plural
	plural -> corpora
	set -> is
	set -> a
	set -> documents
	sentences -> or
	sentences -> sometimes
	sentences -> individual
	documents -> sentences
	"hand-annotated" -> that
	"hand-annotated" -> have
	"hand-annotated" -> been
	documents -> "hand-annotated"
	values -> the
	values -> correct
	"hand-annotated" -> values
	learned -> to
	learned -> be
	"hand-annotated" -> learned
}
